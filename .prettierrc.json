{
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "printWidth": 100,
    "trailingComma": "es5",
    "importOrder": [
        "server-only",
        "^(react|next?/?([a-zA-Z/]*))$",
        "<THIRD_PARTY_MODULES>",
        "^@/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
        "@trivago/prettier-plugin-sort-imports",
        "prettier-plugin-tailwindcss"
    ]
}
